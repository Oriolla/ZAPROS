= form_for @request do |f|
  - if @request.errors.any?
    #error_explanation
      %h2= "#{pluralize(@request.errors.count, "error")} prohibited this request from being saved:"
      %ul
        - @request.errors.full_messages.each do |message|
          %li= message

  .field
    = f.label :assessment11_id
    = f.number_field :assessment11_id
  .field
    = f.label :assessment12_id
    = f.number_field :assessment12_id
  .field
    = f.label :assessment21_id
    = f.number_field :assessment21_id
  .field
    = f.label :assessment22_id
    = f.number_field :assessment22_id
  .field
    = f.label :ans
    = f.text_field :ans
  .actions
    = f.submit 'Save'
